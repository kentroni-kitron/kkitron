{
  "name": "kkitron",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:passta:dev": "---",
    "passta:dev:api": "nx serve passta-api",
    "<DATABASE>": "-------------------------------------------------------------------------------",
    "db:up": "npm run db:docker && npm run db:dev-migrate && npm run db:create-clients",
    "db:docker": "docker-compose -f tools/docker-env/dev.docker-compose.yml up -d --no-recreate --remove-orphans",
    "db:create-clients": "npm run db:create-client:passta && npm run db:create-client:kkitron-oauth",
    "db:create-client:passta": "prisma generate --schema ./libs/passta-api/data-access-db/src/lib/schema.prisma",
    "db:create-client:kkitron-oauth": "prisma generate --schema ./libs/kkitron-oauth/data-access-db/src/lib/schema.prisma",
    "db:dev-migrate": "npm run db:dev-migrate:passta && npm run db:dev-migrate:kkitron-oauth",
    "db:dev-migrate:passta": "env-cmd --no-override -f .local.env npx prisma migrate dev --schema ./libs/passta-api/data-access-db/src/lib/schema.prisma",
    "db:dev-migrate:kkitron-oauth": "env-cmd --no-override -f .local.env npx prisma migrate dev --schema ./libs/kkitron-oauth/data-access-db/src/lib/schema.prisma",
    "</DATABASE>": "------------------------------------------------------------------------------"
  },
  "prisma": {
    "schema": "libs/passta-api/data-access-db/src/lib/schema.prisma"
  },
  "private": true,
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mercurius": "^10.1.7",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-fastify": "^9.2.1",
    "@prisma/client": "^4.9.0",
    "@swc/helpers": "~0.4.11",
    "axios": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "graphql": "^16.6.0",
    "mercurius": "^10.5.1",
    "prisma": "^4.9.0",
    "prisma-nestjs-graphql": "^17.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/devkit": "15.6.3",
    "@nrwl/eslint-plugin-nx": "15.6.2",
    "@nrwl/jest": "^15.6.3",
    "@nrwl/js": "15.6.3",
    "@nrwl/linter": "15.6.2",
    "@nrwl/nest": "15.6.2",
    "@nrwl/node": "15.6.2",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/nx-plugin": "^15.6.3",
    "@nrwl/webpack": "15.6.2",
    "@nrwl/workspace": "15.6.2",
    "@phenomnomnominal/tsquery": "^5.0.0",
    "@swc-node/register": "^1.4.2",
    "@swc/cli": "~0.1.55",
    "@swc/core": "^1.2.173",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "change-case": "^4.1.2",
    "concurrently": "^7.6.0",
    "env-cmd": "^10.1.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "15.6.2",
    "prettier": "^2.6.2",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  }
}
