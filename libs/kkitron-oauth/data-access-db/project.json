{
  "name": "kkitron-oauth-data-access-db",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/kkitron-oauth/data-access-db/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/kkitron-oauth/data-access-db",
        "tsConfig": "libs/kkitron-oauth/data-access-db/tsconfig.lib.json",
        "packageJson": "libs/kkitron-oauth/data-access-db/package.json",
        "main": "libs/kkitron-oauth/data-access-db/src/index.ts",
        "assets": ["libs/kkitron-oauth/data-access-db/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/kkitron-oauth/data-access-db/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/kkitron-oauth/data-access-db/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": [
    "scope:kkitron-oauth"
  ]
}
